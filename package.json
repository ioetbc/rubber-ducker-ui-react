{
  "name": "rubber-ducker-2",
  "displayName": "rubber-ducker-2",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:rubber-ducker-sidebar",
    "onCommand:rubber-ducker-2.helloWorld",
    "onCommand:rubber-ducker-2.doSomething",
    "onCommand:rubber-ducker-2.refresh",
    "onCommand:rubber-ducker-2.addTodo",
    "onCommand:rubber-ducker-2.authenticate"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rubber-ducker-sidebar-view",
          "title": "rubber-ducker",
          "icon": "media/images/icon.svg"
        }
      ]
    },
    "views": {
      "rubber-ducker-sidebar-view": [
        {
          "type": "webview",
          "id": "rubber-ducker-sidebar",
          "name": "rubber-ducker",
          "icon": "media/images/icon.svg",
          "contextualTitle": "rubber-ducker"
        }
      ]
    },
    "commands": [
      {
        "command": "rubber-ducker-2.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "rubber-ducker-2.doSomething",
        "category": "rubberDucker",
        "title": "DoSomething"
      },
      {
        "command": "rubber-ducker-2.refresh",
        "category": "rubberDucker",
        "title": "Refresh"
      },
      {
        "command": "rubber-ducker-2.addTodo",
        "category": "rubberDucker",
        "title": "AddTodo"
      },
      {
        "command": "rubber-ducker-2.authenticate",
        "category": "rubberDucker",
        "title": "authenticate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "concurrently \"parcel src/webviews/pages/sidebar.js\" \"webpack --watch\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "coverage": "nyc npm run test"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/chai": "^4.3.0",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.180",
    "@types/mocha": "^9.1.0",
    "@types/node": "^14.18.16",
    "@types/polka": "^0.5.4",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "chai": "^4.3.6",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "nyc": "^15.1.0",
    "parcel": "^2.4.1",
    "parcel-bundler": "^1.12.5",
    "rollup": "^2.70.1",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@material/chips": "^13.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^14.1.0",
    "@types/firebase": "^3.2.1",
    "express": "^4.17.3",
    "firebase": "^9.6.10",
    "lodash": "^4.17.21",
    "material-components-web": "^13.0.0",
    "polka": "^0.5.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-firebase-hooks": "^5.0.3",
    "react-icons": "^4.3.1",
    "react-scripts": "^5.0.1",
    "socket.io-client": "^4.4.1",
    "styled-components": "^5.3.5",
    "yn": "^5.0.0"
  }
}
